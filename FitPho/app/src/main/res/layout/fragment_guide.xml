<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/darklogocolor"
    tools:context=".GuideFragment">

    <LinearLayout
        android:id="@+id/top1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:padding="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/star" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:fontFamily="@font/myungbold"
            android:text="즐겨찾기"
            android:textColor="@color/white"
            android:textSize="25sp" />
    </LinearLayout>


    <ImageView
        android:id="@+id/chest"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@color/white"
        android:src="@drawable/camera"
        app:layout_constraintBottom_toTopOf="@+id/chest_txt"
        app:layout_constraintEnd_toStartOf="@+id/shoulder"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/top1"/>

    <TextView
        android:id="@+id/chest_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/myungbold"
        android:text="가슴"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/back"
        app:layout_constraintEnd_toEndOf="@+id/chest"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/chest"
        app:layout_constraintTop_toBottomOf="@+id/chest" />

    <ImageView
        android:id="@+id/back"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@color/white"
        android:src="@drawable/camera"
        app:layout_constraintBottom_toTopOf="@+id/back_txt"
        app:layout_constraintEnd_toStartOf="@+id/arm"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chest_txt" />

    <TextView
        android:id="@+id/back_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/myungbold"
        android:text="등"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/legs"
        app:layout_constraintEnd_toEndOf="@+id/back"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/back"
        app:layout_constraintTop_toBottomOf="@+id/back" />

    <ImageView
        android:id="@+id/legs"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@color/white"
        android:src="@drawable/camera"
        app:layout_constraintBottom_toTopOf="@+id/legs_txt"


        app:layout_constraintEnd_toStartOf="@+id/etc"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/back_txt" />

    <TextView
        android:id="@+id/legs_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="50dp"
        android:fontFamily="@font/myungbold"
        android:text="하체"
        android:textColor="@color/white"

        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/legs"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/legs"
        app:layout_constraintTop_toBottomOf="@+id/legs" />


    <ImageView
        android:id="@+id/shoulder"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@color/white"
        android:src="@drawable/camera"
        app:layout_constraintBottom_toTopOf="@+id/shoulder_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/chest"
        app:layout_constraintTop_toBottomOf="@+id/top1"/>

    <TextView
        android:id="@+id/shoulder_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/myungbold"
        android:text="어깨"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/arm"
        app:layout_constraintEnd_toEndOf="@+id/shoulder"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/shoulder"
        app:layout_constraintTop_toBottomOf="@+id/shoulder" />

    <ImageView
        android:id="@+id/arm"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@color/white"
        android:src="@drawable/camera"
        app:layout_constraintBottom_toTopOf="@+id/arm_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/back"
        app:layout_constraintTop_toBottomOf="@+id/shoulder_txt" />

    <TextView
        android:id="@+id/arm_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/myungbold"
        android:text="팔"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/etc"
        app:layout_constraintEnd_toEndOf="@+id/arm"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/arm"
        app:layout_constraintTop_toBottomOf="@+id/arm" />

    <ImageView
        android:id="@+id/etc"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@color/white"
        android:src="@drawable/camera"
        app:layout_constraintBottom_toTopOf="@+id/etc_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/legs"
        app:layout_constraintTop_toBottomOf="@+id/arm_txt" />

    <TextView
        android:id="@+id/etc_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="50dp"
        android:fontFamily="@font/myungbold"
        android:text="기타"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/etc"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/etc"
        app:layout_constraintTop_toBottomOf="@+id/etc" />
</androidx.constraintlayout.widget.ConstraintLayout>